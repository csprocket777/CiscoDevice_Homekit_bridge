{
  "name": "@3p-future-solutions/homebridge-cisco-collaboration-devices",
  "version": "1.0.0",
  "description": "A device bridge for Apple's Homekit framework (via Homebridge.io) supporting Cisco Collaboration devices.",
  "main": "dist/index.js",
  "publishConfig": {
    "@3p-future-solutions:registry": " https://hr-tm-cms-git.cisco.com/api/v4/projects/122/packages/npm/",
    "access": "public"
  },
  "homepage": "https://hr-tm-cms-git.cisco.com/collaboration_device_macros/ciscodevice_homekit_bridge",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run build",
    "postpublish": "npm run clean",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc && concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "start": "tsc && node dist/main.js",
    "watch-and-run": "tsc -w & nodemon -q -w dist dist/index.js",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "homebridge-plugin"
  ],
  "author": "Chuck Shipman <chshipma@cisco.com>",
  "engines": {
    "homebridge": ">=1.0.0"
  },
  "license": "ISC",
  "files": [
    "LICENSE",
    "dist",
    "source"
  ],
  "repository": {
    "type": "git",
    "url": "git@hr-tm-cms-git.cisco.com:collaboration_device_macros/ciscodevice_homekit_bridge.git"
  },
  "bugs": {
    "url": "https://hr-tm-cms-git.cisco.com/collaboration_device_macros/ciscodevice_homekit_bridge/-/issues"
  },
  "devDependencies": {
    "@types/node": "10.17.19",
    "concurrently": "^7.1.0",
    "homebridge": "^1.0.4",
    "nodemon": "^2.0.16",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@eclass/semantic-release-sentry-releases": "^2.9.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/gitlab": "^6.2.1",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "conventional-changelog-conventionalcommits": "^4.6.0",
    "jsxapi": "^5.1.1",
    "semantic-release": "^17.4.4"
  }
